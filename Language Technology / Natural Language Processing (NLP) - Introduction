      "source": [
        "# Accounting the frequancy word per each tweet.\n",
        "term_frequency ={}\n",
        "\n",
        "\n",
        "for document in tok: #Extracting each tweet from document.\n",
        "  word_freq=0\n",
        "\n",
        "  for i in range(len(filtered_tokens)): #Getting most common word from filtered_tokens\n",
        "  \n",
        "    if filtered_tokens[i][0] in document: #Counting each most common word with each tweet\n",
        "      word_freq+=1\n",
        "  term_frequency[document] = word_freq\n",
        "\n",
        "term_frequency\n"
      ],
