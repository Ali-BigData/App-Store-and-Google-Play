      "source": [
        "# Text Segmentation"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "kGaACQErd7pj",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "5b0e2c3b-f0a1-4bf6-9af1-d2927f2a17f7"
      },
      "source": [
        "# Tokenization / word segmentation: Segment text into individual tokens\n",
        "\n",
        "import re\n",
        "\n",
        "tok=[]\n",
        "for x in list_tweets:\n",
        "\n",
        "  tokenized = re.sub(r'([.,!?]+)', r' \\1', x) # replace . , ! ? with whitespace+character(s), '+' means one or more\n",
        "  tokenized = re.sub(r\"(n’t)\", r\" \\1\", tokenized) # clitics\n",
        "\n",
        " \n",
        "  \n",
        "  tokenized =  re.sub(r\"http\\S+\", \"\", tokenized) # Removing (https:\\t\\.co )\n",
        "  tokenized=  re.sub(r\"\\.co\\S+\", \"\", tokenized) # Removing (https:\\t\\.co )\n",
        "  tokenized = tokenized.replace('!','') \n",
        "  \n",
        "  tok.append(tokenized)\n",
        "  \n",
        "tok"
      ],
      "execution_count": null,
